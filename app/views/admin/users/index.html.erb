<header>
    <h1>Users</h1>
    <ul class="actions">
        <li>
            <%= link_to "Spawn user", new_admin_user_path, class: "new" %>
        </li>
    </ul>
</header>

<ul>
    <% @users.each do |user| %>
    <li>
        <%= link_to user, [:admin,user] %>
         &nbsp;-&nbsp;
        <%= link_to "Edit User", edit_admin_user_path(user), class: "edit" %>
    </li>
    <% end %>
</ul>
